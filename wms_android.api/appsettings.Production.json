{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "JwtConfig": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "wms_android.api",
    "Audience": "wms_android.client",
    "ExpiryInDays": 7,
    "ExpirationInMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting": "Warning",
      "Microsoft.AspNetCore.Routing": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "https://www.system.ficma.co.ke",
      "${API_BASE_URL}",
      "http://localhost:3000",
      "https://localhost:3001"
    ]
  },
  "ApiSettings": {
    "BaseUrl": "${API_BASE_URL}"
  },
  "UseRemoteServices": true
}